{
	"manifest_version": 2,
	"name": "Cancel Discord Support Website",
	"version": "1.1",
	"author": "TechnicJelle",
	"homepage_url": "https://github.com/TechnicJelle/CancelDiscordSupportWebsite",

	"description": "Quickly closes the Discord Support page after being opened through a misclick..",

	"icons": {
		"48": "icons/logo-48.png"
	},

	"permissions": [
		"webNavigation",
		"tabs"
	],

	"background": {
		"scripts": ["script.js"],
		"persistent": false,
		"type": "module"
	},

	"browser_specific_settings": {
		"gecko": {
			"id": "CancelDiscordSupportWebsite@TechnicJelle.com"
		}
	}
}
